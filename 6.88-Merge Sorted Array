class Solution {
public:
    void merge(vector<int>& nums1, int m, vector<int>& nums2, int n) {
        vector<int> v(m+n);
        for(int i = 0;i<m;i++){
            v[i]=nums1[i];
        }
        for(int i = 0;i<n;i++){
            v[m+i] = nums2[i];
        }
      sort(v.begin(),v.end());
        
        for(int i = 0;i<m+n;i++){
            nums1[i]=v[i];
        }
    }
    
};
